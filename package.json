{
  "name": "cubingusa",
  "version": "1.0.0",
  "description": "CubingUSA frontend",
  "private": true,
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "site": "source env/bin/activate && pip install -r requirements.txt && $(gcloud beta emulators datastore env-init) && gunicorn -b :8083 app.flask:app --reload $(find app/templates -type f -name '*.html' -exec echo --reload-extra-file {} \\;)",
    "sass": "./run_sass.sh",
    "all": "concurrently --kill-others \"npm run site\" \"npm run sass\" \"npx webpack --watch\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubingusa/org.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cubingusa/org/issues"
  },
  "homepage": "https://github.com/cubingusa/org#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-react-constant-elements": "^7.24.7",
    "@babel/plugin-transform-react-inline-elements": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bower": "^1.8.14",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@wca/helpers": "^1.1.5",
    "bootstrap": "^5.3.3",
    "react-bootstrap": "^2.10.2",
    "react-router-dom": "^6.23.1"
  },
  "lint-staged": {
    "*.tsx": "prettier --write"
  }
}
