{
  "name": "cubingusa",
  "version": "1.0.0",
  "description": "CubingUSA frontend",
  "private": true,
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "site": "source env/bin/activate && pip install -r requirements.txt && $(gcloud beta emulators datastore env-init) && gunicorn -b :8083 app.flask:app --reload $(find app/templates -type f -name '*.html' -exec echo --reload-extra-file {} \\;)",
    "sass": "./run_sass.sh",
    "all": "concurrently --kill-others \"npm run site\" \"npm run sass\" \"npx webpack --watch\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubingusa/org.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cubingusa/org/issues"
  },
  "homepage": "https://github.com/cubingusa/org#readme",
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "protobufjs-loader": "^2.0.2",
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@wca/helpers": "^1.1.5",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "luxon": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "lint-staged": {
    "*.tsx": "prettier --write"
  }
}
